<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.pgy.mapper.TypeMapper">
	
	
	
	
	<select id="getTypeById" resultMap="typeBaseResultMap">
		
		select * from p_type where typeId = #{typeId};
		
		
	</select>
	
	
	
	
	
	<!-- 定义ResultMap -->
	<resultMap id="typeBaseResultMap" type="type" autoMapping="true"></resultMap>
	
</mapper>